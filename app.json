{
  "expo": {
    "name": "app-test",
    "slug": "app-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jefmbor.apptest"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jefmbor.apptest"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "40.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ]
  }
}